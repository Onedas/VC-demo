    <!-- Custom styles for this template -->
    <link href="css/clean-blog.css" rel="stylesheet">

</head>

<body>
    <!-- Page Header -->
    <header class="masthead" style="background-image: url('img/home-bg.jpg')">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 col-md-10 mx-auto">
                    <div class="site-heading">
                        <h1>VC Demo</h1>
                        <span class="subheading">
                            This is the demo page for VC evaluation.
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- Main Content -->
    <div class="container">
        <div class="row">
            <div class="col-lg-14 col-md-12 mx-auto">
                <!-- Summary -->
                <div class="post-preview">
                    <h2 class="post-title">
                        Summary
                    </h2>
                    <p>
                        TBW
                    </p>
                </div>


                <!-- Audios -->
                <div class="post-preview">
                    <h2 class="post-title">
                        Naturalness
{#                        One-shot Voice Conversion#}
                    </h2>
                    <h3 class="post-meta">
{#                        voice conversion from unseen speaker to unseen speaker#}
                    </h3>
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">No.</th>
                                <th scope="col">Audios</th>
                                <th scope="col">Survey Form</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for i, a, b, c, d, e, f, survey in naturalness_rows%}
                            <tr>
                                <td>
                                    # {{ i }}
                                </td>
                                <td>
                                    <table>
                                        <tbody>
                                          <tr>
                                            <td>A</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ a }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>B</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ b }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>C</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ c }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>D</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ d }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>E</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ e }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>F</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ f }}">
                                                </audio>
                                            </td>
                                          </tr>
                                        </tbody>
                                    </table>
                                </td>
                                <td>
                                    <iframe src="{{ survey }}/viewform?embedded=true" width="640" height="640">로드 중…</iframe>
                                </td>
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
                <hr>

                <!-- Audios2 -->
                <div class="post-preview">
                    <h2 class="post-title">
                        Similarity Evaluation
{#                        One-shot Voice Conversion#}
                    </h2>
                    <h3 class="post-meta">
{#                        voice conversion from unseen speaker to unseen speaker#}
                    </h3>
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">No. </th>
                                <th scope="col">Target</th>
                                <th scope="col">Audios</th>
{#                                <th scope="col">Survey Form</th>#}
                            </tr>
                        </thead>
                        <tbody>
                            {% for i, tgt, a, b, c, d, e, survey in similarity_rows%}
                            <tr>
                                <td>
                                    {{ i }}
                                </td>
                                <td>
                                    <audio controls preload="none">
                                        <source src="{{ tgt }}">
                                    </audio>
                                </td>
                                <td>
                                    <table>
                                        <tbody>
                                          <tr>
                                            <td>A</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ a }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>B</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ b }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>C</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ c }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>D</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ d }}">
                                                </audio>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td>E</td>
                                            <td>
                                                <audio controls preload="none">
                                                    <source src="{{ e }}">
                                                </audio>
                                            </td>
                                          </tr>
                                        </tbody>
                                    </table>
                                </td>
                                <td>
                                    <iframe src="{{ survey }}/viewform?embedded=true" width="640" height="640">로드 중…</iframe>
                                </td>
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <!-- Footer -->
        <foo ter>
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-md-10 mx-auto">
                        <ul class="list-inline text-center">
